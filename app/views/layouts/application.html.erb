<!DOCTYPE html>
<html>
  <head>
    <title>Shrubhub</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <header class='clearfix'>
      <div class="nav">
        <p><%= link_to "ShrubHub", :root %></p>
        <p><%= link_to "Sell a Plant!", new_plant_path %></p>
        <p><%= link_to "Cart", cart_path %></p>
    </header>

    <div class='content'>
      <%= yield %>
    </div>

  </body>
</html>
